{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "FromDevz Admin Panel",
  "mainBinaryName": "FromDevz Admin Panel",
  "version": "1.0.0",
  "identifier": "com.admin-panel-v1.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "createUpdaterArtifacts": true
  },
  "plugins": {
    "updater": {
      "windows": {
        "installMode": "basicUi"
      },
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEM0NTYxOThDMUM1MTZFMkQKUldRdGJsRWNqQmxXeE51U1NLYXEwOFU5K3BSRXQwZjZIQ1BnR2drMHZPSkZpcEYxT0JYTE5CN0YK",
      "endpoints": [
        "https://api.github.com/repos/Fran249/Admin-Panel-for-CXP/releases/latest/latest.json"
      ]
    }
  },
  "app": {
    "windows": [
      {
        "title": "FromDevz Admin Panel",
        "fullscreen": false,
        "resizable": true,
        "width": 1280,
        "height": 720,
        "minWidth": 1280,
        "minHeight": 720
      }
    ],
    "security": {
      "csp": null,
      "capabilities": []
    }
  }
}